{
  "name": "visual-bookmarks-dev",
  "version": "1.0.0",
  "description": "Visual bookmarks",
  "scripts": {
    "lint:es": "eslint src/js/**",
    "lint:css": "stylelint src/css/**;  exit 0",
    "lint:html": "htmlhint src/*.html; exit 0",
    "lint": "npm run lint:es && npm run lint:css && npm run lint:html",
    "start": "webpack --mode=development --watch",
    "build": "webpack --mode=production",
    "zip": "bestzip extension.zip extension/*",
    "release": "npm run build && npm run zip",
    "locales": "node autocompleteLocales.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:k-ivan/visual-bookmarks-chrome.git"
  },
  "browserslist": [
    "chrome >= 80"
  ],
  "author": "ivan_k",
  "license": "ISC",
  "homepage": "https://github.com/k-ivan/visual-bookmarks-chrome#readme",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "bestzip": "^2.1.7",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.0.0",
    "css-loader": "^6.2.0",
    "eslint": "^7.22.0",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "htmlhint": "^1.1.3",
    "mini-css-extract-plugin": "^2.2.1",
    "postcss": "^8.2.10",
    "postcss-csso": "^5.0.0",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^5.2.0",
    "postcss-nesting": "^7.0.1",
    "postcss-sort-media-queries": "^3.6.5",
    "raw-loader": "^4.0.2",
    "style-loader": "^3.2.1",
    "stylelint": "^15.10.1",
    "stylelint-config-standard": "^25.0.0",
    "svg-spritemap-webpack-plugin": "^4.3.3",
    "terser-webpack-plugin": "^5.1.1",
    "to-string-loader": "^1.1.6",
    "webpack": "^5.26.1",
    "webpack-cli": "^4.5.0",
    "webpackbar": "^5.0.0-3"
  },
  "dependencies": {
    "fast-average-color": "^6.4.2",
    "form-validation-plugin": "^0.0.11",
    "glory-modal": "^2.3.1",
    "idb": "^7.0.1",
    "sortablejs": "^1.8.4",
    "tabs-slider": "^3.1.1"
  }
}
